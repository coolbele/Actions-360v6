
config wifi-device 'radio0'
	option type 'mac80211'
	option macaddr 'b8:80:35:6b:75:00'
	option num_global_macaddr '8'
	option country 'US'
	option legacy_rates '0'
	option mu_beamformer '0'
	option band '5g'
	option txpower '19'
	option htmode 'HE80'
	option channel '149'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'abc5124937,'
	option ssid 'YM520－5G'

config wifi-device 'radio1'
	option type 'mac80211'
	option macaddr 'b8:80:35:6b:75:01'
	option band '2g'
	option htmode 'HE20'
	option num_global_macaddr '8'
	option country 'US'
	option legacy_rates '0'
	option mu_beamformer '0'
	option channel '8'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'abc5124937,'
	option ssid 'YM520－2.4G'

