
config wifi-device 'wifi0'
	option type 'qcawificfg80211'
	option channel '149'
	option macaddr 'b8:80:35:6b:75:01'
	option hwmode '11axa'
	option htmode 'HT80'
	option country 'US'
	option txpower '24'

config wifi-iface 'ath0'
	option device 'wifi0'
	option network 'lan'
	option mode 'ap'
	option wmm '1'
	option rrm '1'
	option qbssload '1'
	option ssid 'YM520-5G'
	option sae '1'
	option encryption 'psk-mixed+ccmp'
	option key 'abc5124937,'

config wifi-device 'wifi1'
	option type 'qcawificfg80211'
	option macaddr 'b8:80:35:6b:75:00'
	option hwmode '11axg'
	option htmode 'HT40'
	option country 'US'
	option txpower '24'
	option channel '6'

config wifi-iface 'ath1'
	option device 'wifi1'
	option network 'lan'
	option mode 'ap'
	option wmm '1'
	option rrm '1'
	option qbssload '1'
	option ssid 'YM520-2.4G'
	option sae '1'
	option encryption 'psk-mixed+ccmp'
	option key 'abc5124937,'

